formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="Suppliers" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort><column index="1" mode="0"/></sort><column_widths><column index="1" value="73"/><column index="2" value="135"/><column index="3" value="300"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">-- Soru 1: Tum musterilerin listesini alin. 
select * from Customers

-- Soru 2: Almanya'daki musterileri listeleyin. 
SELECT ContactName, Country FROM Customers WHERE Country = 'Germany'; 

-- Soru 3: Mevcut urunlerin adlarini ve fiyatlarini listeleyin. 
select ProductName , Price from Products

-- Soru 4: Urunleri fiyata gore azalan sirayla listeleyin.  
SELECT  ProductName, Price FROM Products ORDER BY Price DESC; 

-- Soru 5: Ilk 10 urunu ada gore listeleyin. 
SELECT ProductName FROM Products LIMIT 10; 

-- Soru 6: Urun fiyatlarini ortalama fiyatla karsilastirin. 
SELECT ProductName, Price FROM Products WHERE Price &gt; (SELECT avg(Price) FROM 
Products); 

-- Soru 7: Her siparisin urun adedini listeleyin. 
SELECT OrderID, SUM(Quantity) AS TotalItems FROM OrderDetails GROUP BY OrderID; 

-- Soru 8: Her calisanin aldigi siparis sayisini bulun.
SELECT e.FirstName, e.LastName, COUNT(o.OrderID) AS OrderCount 
FROM Employees e 
JOIN Orders o ON e.EmployeeID = o.EmployeeID 
GROUP BY e.FirstName, e.LastName;  

-- Soru 9: Urunleri kategoriye gore gruplayin ve ortalama fiyatlarini hesaplayin. 
select c.CategoryName , avg(p.Price) as AvgPrice from Categories c
JOIN Products p on c.CategoryID = p.CategoryID 
GROUP by c.CategoryName
</sql><current_tab id="0"/></tab_sql></sqlb_project>
